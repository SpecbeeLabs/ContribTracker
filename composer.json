{
    "name": "axelerant/contrib-tracker",
    "description": "Contribution Tracker for Organizations",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "hussainweb",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/admin_toolbar": "^1.0",
        "drupal/better_exposed_filters": "3.x-dev",
        "drupal/bootstrap": "^3.0",
        "drupal/config_installer": "^1.0",
        "drupal/console": "^1.0.1",
        "drupal/core": "^8.5",
        "drupal/field_permissions": "^1.0",
        "drupal/flag": "^4.0",
        "drupal/inline_entity_form": "^1.0",
        "drupal/raven": "^2.6",
        "drupal/redis": "^1.0",
        "drupal/slack": "1.x-dev",
        "drupal/social_auth": "^2.0",
        "drupal/social_auth_google": "^2.0",
        "drush/drush": "~9.0",
        "google/apiclient": "^2.0",
        "hussainweb/drupal-composer-helper": "^1.0",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "~1.3"
    },
    "require-dev": {
        "geerlingguy/drupal-vm": "^4.6",
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patches": {
        },
        "drupal-composer-helper": {
            "web-prefix": "web"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"]
        },
        "merge-plugin": {
            "include": [
                "web/modules/custom/contrib_tracker/composer.json"
            ]
        },
        "enable-patching": true
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "apcu-autoloader": true
    }
}
